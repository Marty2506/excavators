{% extends "layout.j2" %}

{% set page = "search-results" %}

{% block content %}
  <main class="page-main search-page">
    <div class="container">
      <div class="search-page__breadcumbs breadcumbs breadcumbs--blue">
        <ul class="breadcumbs__list">
          <li class="breadcumbs__item breadcumbs__item--home">
            <a class="breadcumbs__link breadcumbs__link--home" href="index.html">
              <svg class="breadcumbs__link-icon" width="19" height="17" viewBox="0 0 19 17">
                <use xlink:href="img/sprite.svg#home-icon"></use>
              </svg>
              <span>На Главную</span>
            </a>
          </li>
          <li class="breadcumbs__item">
            <span class="breadcumbs__link breadcumbs__link--current">Результаты поиска</span>
          </li>
        </ul>
      </div>
    </div>
    <section class="search-page__results">
      <div class="container">
        <h1 class="search-page__title section-header">Результаты поиска</h1>
        <div class="search-page__cards cards">
          <ul class="search-page__cards-list cards__list">
            <li class="cards__item">
              <div class="product-card product-card--sale product-card--new product-card--search-result">
                <div class="product-card__content">
                  <a class="product-card__link" href="./product-page.html">
                    <span class="visually-hidden">Перейти на страницу товара</span>
                  </a>
                  <div class="product-card__attributes">
                    <span class="product-card__attribute product-card__attribute--new">Новинка</span>
                    <span class="product-card__attribute product-card__attribute--sale">-20%</span>
                  </div>
                  <figure class="product-card__figure">
                    <picture>
                      <source type="image/webp" srcset="@img/product-card-image2.webp 1x">
                      <img class="product-card__image" src="@img/product-card-image2.png" alt="">
                    </picture>
                  </figure>
                  <h3 class="product-card__title">Универсальный гусеничный экскаватор XE700C</h3>
                </div>
              </div>
            </li>
            <li class="cards__item">
              <div class="product-card product-card--sale product-card--new product-card--search-result">
                <div class="product-card__content">
                  <a class="product-card__link" href="./product-page.html">
                    <span class="visually-hidden">Перейти на страницу товара</span>
                  </a>
                  <div class="product-card__attributes">
                    <span class="product-card__attribute product-card__attribute--new">Новинка</span>
                    <span class="product-card__attribute product-card__attribute--sale">-20%</span>
                  </div>
                  <figure class="product-card__figure">
                    <picture>
                      <source type="image/webp" srcset="@img/product-card-image2.webp 1x">
                      <img class="product-card__image" src="@img/product-card-image2.png" alt="">
                    </picture>
                  </figure>
                  <h3 class="product-card__title">Универсальный гусеничный экскаватор XE700C</h3>
                </div>
              </div>
            </li>
            <li class="cards__item">
              <div class="product-card product-card--sale product-card--new product-card--search-result">
                <div class="product-card__content">
                  <a class="product-card__link" href="./product-page.html">
                    <span class="visually-hidden">Перейти на страницу товара</span>
                  </a>
                  <div class="product-card__attributes">
                    <span class="product-card__attribute product-card__attribute--new">Новинка</span>
                    <span class="product-card__attribute product-card__attribute--sale">-20%</span>
                  </div>
                  <figure class="product-card__figure">
                    <picture>
                      <source type="image/webp" srcset="@img/product-card-image2.webp 1x">
                      <img class="product-card__image" src="@img/product-card-image2.png" alt="">
                    </picture>
                  </figure>
                  <h3 class="product-card__title">Универсальный гусеничный экскаватор XE700C</h3>
                </div>
              </div>
            </li>
          </ul>
        </div>
      </div>
    </section>
    {# {% include "./blocks/leasing-header.j2" %} #}
  </main>
{% endblock content %}

{% block js %}
  <script src="./js/jquery-3.4.1.slim.js"></script>
  <script src="./js/slick.min.js"></script>
  <script src="./js/tabs.min.js"></script>
  <script src="./js/custom-select.min.js"></script>
  <script src="./js/menu.min.js"></script>
  <script src="./js/burger.min.js"></script>
  <script src="./js/main.min.js"></script>
{% endblock %}
